<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.gvozdberi" version="1.0.20" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>GvozdBeri</name>
    <description>
        Гвоздь Бери мобильный каталог
    </description>
    <author email="info@softf1.ru" href="https://softf1.ru">
        SoftF1 Developer
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
    <platform name="android">
        <icon density="ldpi" src="www/res/icon/xxxhdpi.png"/>
        <icon density="mdpi" src="www/res/icon/xxxhdpi.png"/>
        <icon density="hdpi" src="www/res/icon/xxxhdpi.png"/>
        <icon density="xhdpi" src="www/res/icon/xxxhdpi.png"/>
        <icon density="xxhdpi" src="www/res/icon/xxxhdpi.png"/>
        <icon density="xxxhdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="land-ldpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="land-mdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="land-hdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="land-xhdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="land-xxhdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="land-xxxhdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="port-ldpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="port-mdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="port-hdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="port-xhdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="port-xxhdpi" src="www/res/icon/xxxhdpi.png"/>
        <splash density="port-xxxhdpi" src="www/res/icon/xxxhdpi.png"/>
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
